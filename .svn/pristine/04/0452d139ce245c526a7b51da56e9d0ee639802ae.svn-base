}
.toolbar_r a:hover {
  color: #e53;
}
.toolbar_r .r {
  width: 15px;
  height: 13px;
  background: url("../../images/toolbar.png") no-repeat -59px 0;
  cursor: pointer;
}

/*****header****/
.top_auto {
  padding: 15px 0;
  height: 100px;
}

.logo {
  width: 240px;
  height: 100px;
}
.logo img {
  width: 240px;
  height: 100px;
}

.top_search {
