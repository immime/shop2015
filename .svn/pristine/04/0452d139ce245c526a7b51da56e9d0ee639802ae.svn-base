@charset "UTF-8";
* {
  margin: 0;
  padding: 0;
}

body {
  margin: 0 auto;
  font-size: 12px;
  position: static;
  color: #000;
  font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
}

ul, dl, dd, h1, h2, h3, h4, h5, h6, form, p, input {
  margin: 0;
  padding: 0;
}

em, i {
  font-style: normal;
}

img {
  border: none;
}

ul {
  list-style: none;
}

/*****common****/
.w1190, .toolbar_auto, .top_auto, .nav_auto, #listcontainer {
  width: 1190px;
}

.w990, .foot_s_body, .link_auto, .footer_auto {
  width: 990px;
}

.mrauto, .toolbar_auto, .top_auto, .nav_auto, .foot_s_body, .link_auto, .footer_auto, #listcontainer {
  margin-left: auto;
  margin-right: auto;
}

.inline-block, .collect em, .city_l em, .toolbar_r .r, .logo, .top_search, .search_s, .search_btn, .top_ad {
  display: inline-block;
  *display: inline;
  zoom: 1;
  vertical-align: middle;
}

.text-overflow {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/***media query***/
@media screen and (max-width: 1190px) {
  .w1190, .toolbar_auto, .top_auto, .nav_auto, #listcontainer {
    width: 990px;
  }
}
a {
  color: #666;
  text-decoration: none;
}
a:hover {
  color: #e53;
  text-decoration: underline;
}

button, [type=button], [type=submit] {
  border: none;
  background: none;
}

a, area {
  blr: expression(this.onFocus=this.blur());
}

input {
  outline: 0;
}

a:focus {
  outline: 0;
}

.clear {
  clear: both;
}

.left {
  float: left;
}

.right {
  float: right;
}

.none {
  display: none;
}

.clearfix:after, .breadcrumb:after, #listcontent:after, #listcontent .brands:after, #listcontent .filter_container .filter_item:after, #listcontent .filter_container .filter_con_list:after, #listcontent .filter_container .filter_con_list .brand_letters:after, #listcontent .product_container .product_list:after, #listcontent .product_container .product_list li .product_status:after {
  display: block;
  font-size: 0;
  content: ".";
  clear: both;
  height: 0;
  visibility: hidden;
}

.clearfix, .breadcrumb, #listcontent, #listcontent .brands, #listcontent .filter_container .filter_item, #listcontent .filter_container .filter_con_list, #listcontent .filter_container .filter_con_list .brand_letters, #listcontent .product_container .product_list, #listcontent .product_container .product_list li .product_status {
  *height: 1%;
}

.f14, .link_auto dl dt {
  font-size: 14px;
}

.mt5 {
  margin-top: 5px;
}

.mt10 {
  margin-top: 10px;
}

.mt15 {
  margin-top: 15px;
}

.mt20 {
  margin-top: 20px;
}

.mt25 {
  margin-top: 25px;
}

.mt30 {
  margin-top: 30px;
}

/*****toolbar****/
#toolbar {
  height: 28px;
  line-height: 28px;
  border-bottom: 1px solid #ddd;
}

.toolbar_auto {
  font-family: "SimSun";
  overflow: hidden;
}

.collect {
  color: #333;
  padding-right: 20px;
}
.collect em {
  width: 14px;
  height: 14px;
  background: url("../../images/toolbar.png") no-repeat 0 0;
  margin-right: 5px;
}

.city_l em {
  width: 9px;
  height: 15px;
  background: url("../../images/toolbar.png") no-repeat -31px 0;
  margin-right: 5px;
}
.city_l strong {
  color: #e53;
}

.city_r {
  padding-left: 10px;
