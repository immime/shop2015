  left: 0;
  top: 0;
}

.brand_2 {
  left: 200px;
  top: 0;
}

.brand_3 {
  left: 200px;
  top: 140px;
}

.brand_4 {
  left: 400px;
  top: 0;
}

.brand_5 {
  left: 600px;
  top: 0;
}

.brand_6 {
  left: 600px;
  top: 140px;
}

.brand_7 {
  left: 800px;
  top: 0;
}

.brand_8 {
  left: 1000px;
  top: 0;
}

.brand_9 {
  left: 1000px;
  top: 140px;
}

/*狗狗商品*/
.dog_top {
  border-bottom: 1px solid #e53;
  height: 38px;
}

